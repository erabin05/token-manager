{
  "name": "token-manager-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "node esbuild.config.cjs",
    "build:watch": "node esbuild.config.cjs --watch",
    "start": "node dist/server.js",
    "db:build": "node esbuild.config.cjs",
    "db:seed": "npm run build && node dist/seed.js",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio"
  },
  "dependencies": {
    "@fastify/type-provider-typebox": "^5.2.0",
    "@prisma/client": "^6.12.0",
    "@types/node": "^24.1.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "esbuild": "^0.20.0",
    "ts-node-dev": "^2.0.0"
  }
}
